{
  "page-anchor": {
    "parent3d": "screen 1.25 center"
  },

  "anchor-cube": {
    "object3d": {
      "type": "cube",
      "size": "10px",
      "color": "#FF0000"
    },
    "position3d": "rotateY(0deg)"
  },

  "latin-panel": {
    "parent3d": "main-dock",
    "position3d": "translate(0px, 0px, 0px) rotateY(-30deg)",
    "display3d": "surface",
    "scaling3d": "screen",
    "surface3d": [{
      "type": "circular",
      "radius-outer": "200px",
      "angle": "full"
    }]
  },

  "latin-cylinder": {
    "parent3d": "main-dock",
    "position3d": "translate(0px, 0px, 0px) rotateY(-30deg)",
    "display3d": "surface",
    "scaling3d": "screen",
    "surface3d": [{
      "type": "cylindrical",
      "radius-outer": "50px",
      "before": "translate(0px, 0px, 0px) rotate(90deg)",
      "after": "translate(0px, 0px, 0px) rotate(90deg)"
    }]
  },

  "main-content": {
    "parent3d": "main-dock",
    "position3d": "translate(100px, -100px, 0px) rotateY(-60deg)",
    "display3d": "surface",
    "scaling3d": "screen",
    "surface3d": [{ "type": "rectangular" }]
  },

  "child-content": {
    "parent3d": "main-content",
    "position3d": "translate(100px, -200px, 100px) rotateY(0deg)",
    "display3d": "surface",
    "scaling3d": "screen",
    "surface3d": [{ "type": "rectangular" }]
  },

  "child-panel": {
    "parent3d": "main-content",
    "position3d": "rotateX(0deg) translate(0px, 0px, 0px)",
    "display3d": "surface",
    "scaling3d": "screen",
    "surface3d": [{ "type": "rectangular" }]
  }
}
